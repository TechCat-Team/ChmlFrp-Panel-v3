    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "highlight.js": "^11.11.1",
    "naive-ui": "^2.41.0",
    "pinia": "^3.0.2",
    "skinview3d": "^3.4.1",
    "three": "^0.176.0",
